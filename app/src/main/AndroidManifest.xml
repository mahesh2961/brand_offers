<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="adv.brand.com.lavanya">


    <uses-permission android:name="android.permission.INTERNET" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <uses-permission android:name="android.permission.CALL_PHONE" />


    <application
        android:allowBackup="true"
        android:icon="@drawable/splash_logo"
        android:label="@string/app_name"
        android:name=".BrandApp"
        android:theme="@style/AppTheme">
        <activity android:name="adv.brand.com.lavanya.MainActivity" android:screenOrientation="portrait">
            <!--<intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>-->
        </activity>

        <activity android:name=".StoreProfileActivity" android:screenOrientation="portrait"/>
        <activity android:name=".OfferDetailsActivity" android:screenOrientation="portrait"/>
        <activity android:name=".FilterActivity" android:screenOrientation="portrait"/>
        <activity android:name=".WebviewActivity" android:screenOrientation="portrait"/>


        <activity android:name=".SplashActivity" android:screenOrientation="portrait"
            android:theme="@style/SplashTheme">
            <intent-filter>
            <action android:name="android.intent.action.MAIN" />
            <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>